<%- model_class = AlarmNotification -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:tank_name) %>:</strong></dt>
  <dd><%= @alarm_notification.tank_name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:alarm_level) %>:</strong></dt>
  <dd><%= @alarm_notification.alarm_level %></dd>
  <dt><strong><%= model_class.human_attribute_name(:alert_owner) %>:</strong></dt>
  <dd><%= @alarm_notification.alert_owner %></dd>
  <dt><strong><%= model_class.human_attribute_name(:message_part1) %>:</strong></dt>
  <dd><%= @alarm_notification.message_part1 %></dd>
  <dt><strong><%= model_class.human_attribute_name(:message_part2) %>:</strong></dt>
  <dd><%= @alarm_notification.message_part2 %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              alarm_notifications_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_alarm_notification_path(@alarm_notification), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              alarm_notification_path(@alarm_notification),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
